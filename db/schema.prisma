datasource db {
    url      = env("DATABASE_URL")
    provider = "postgresql"
}

model Asset {
    id        String   @id
    name      String   @unique @db.VarChar(64)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    // Placable  Placable[]
}

